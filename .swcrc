[
    {
        "test": ".tsx?$",
        "jsc": {
            "parser": {
                "syntax": "typescript",
                "tsx": true,
                "decorators": false,
                "dynamicImport": true
            },
            "target": "es2022"
        },
        "module": {
            "type": "es6"
        }
    },
    {
        "test": ".jsx?$",
        "jsc": {
            "parser": {
                "syntax": "ecmascript",
                "jsx": true,
                "decorators": false,
                "dynamicImport": true
            }
        }
    }
]
